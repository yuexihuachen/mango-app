{
  "name": "mango-pp",
  "version": "1.0.0",
  "description": "light-template",
  "scripts": {
    "dev": "pnpm run --filter '*' dev",
    "client:build": "pnpm -F './client' build",
    "client:build:dev": "pnpm -F './client' build:dev",
    "server:dev": "bun run ./server/src/index.ts",
    "server:debug": "pnpm -F './server' debug",
    "start": "bun --env-file=.env.production run ./server/src/index.ts"
  },
